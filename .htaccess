<Files ".env">
    Require all denied
</Files>

<Files ".git">
    Require all denied
</Files>

<Files ".sql">
    Require all denied
</Files>

<IfModule mod_authz_core.c>
    Require all granted
</IfModule>
<IfModule !mod_authz_core.c>
    Order allow,deny
    Allow from all
</IfModule>

Options -Indexes
